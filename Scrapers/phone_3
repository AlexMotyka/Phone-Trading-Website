<script type="text/javascript">

    

    $(document).ready(function() {

        function checkCookieAndTrackGA(cookieName, action) {
            var cookie = $.cookie(cookieName);
            if (cookie !== undefined) {
                action();
                $.removeCookie(cookieName, {path: '/' });
            }
        }

        

        checkCookieAndTrackGA("postAd", Kj.Tracking.initTrackPostAdSuccess);
        checkCookieAndTrackGA("editAd", Kj.Tracking.initTrackEditAdSuccess);

        
            Kj.PostingLimit.Tracking.init();
        
    });

</script>
